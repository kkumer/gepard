# robodoc.rc for project gepard documentation

items:
    NAME
    SYNOPSIS
    FILE DESCRIPTION
    DESCRIPTION
    INPUTS
    OUTPUT
    RETURN VALUE
    IDENTIFIERS
    EXAMPLE
    NOTES
    BUGS
    TODO
    SEE ALSO
    CHILDREN
    PARENTS
    MEMBER OF
    SOURCE
source items:
    SYNOPSIS
options:
    --src ./src 
#    --nodesc
    --sections
headertypes:
    h "File" file
    p "Program" program
    s "Subroutine" subroutine
    f "Function" function
    c "Common Block" commonblock
    v "Variable" variable
